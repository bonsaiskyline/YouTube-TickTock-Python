{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
	  "ghcr.io/devcontainers/features/github-cli:1": {
			"version": "2.25.1"
		},
		"ghcr.io/devcontainers-contrib/features/poetry:2": {
			"version": "1.4.1"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"GitHub.copilot",
				"ms-python.python",
				"ms-python.pylint"
			]
		}
	},
	"remoteEnv": {
		"GOOGLE_APPLICATION_CREDENTIALS": "./cred.json"
	}
}